		  <dependency id="Com.Airbnb.iOS.Lottie" version="1.0.0.7" />
       </group>
	   </dependencies>
    </metadata>
    
	<files>
		<file src="LottieDialogs.iOS\bin\Release\LottieDialogs.iOS.dll" target="lib\xamarinios\LottieDialogs.iOS.dll" />
        <file src="LottieDialogs.Android\bin\Release\LottieDialogs.Android.dll" target="lib\monodroid\LottieDialogs.Android.dll" />
		<file src="LottieDialogs.Abstractions\bin\Release\LottieDialogs.Abstractions.dll" target="lib\xamarinios\LottieDialogs.Abstractions.dll"/>
