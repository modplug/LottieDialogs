<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>LottieDialogs</id>
        <version>0.0.5.8</version>
        <title>Lottie Dialogs for Xamarin</title>
        <authors>Erlend Angelsen</authors>
        <owners>Erlend Angelsen</owners>
        <licenseUrl>https://github.com/modplug/LottieDialogs/blob/master/LottieDialogs/LICENSE</licenseUrl>
        <projectUrl>https://github.com/modplug/LottieDialogs</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Lottie Dialogs for Xamarin</description>
